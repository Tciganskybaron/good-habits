{
  "name": "vite-react",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@eslint/js": "9.17.0",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.9",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.2",
    "vite": "6.0.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*": "prettier --write --ignore-unknown"
  }
}
